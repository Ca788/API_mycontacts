{
    "env": {
        "es2021": true,
        "node": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "off"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "eslintlinebreak-style": "off",
        "eslintindent": "off",
       "eslint-disable indent": "off"
    }
}
